<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pb.dada.com.passwordbox">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/img_password_box_iclauncher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <!-- splash页面 -->
        <activity
            android:name="pb.dada.com.passwordbox.ui.activitys.SplashActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- 主页面 -->
        <activty android:name="pb.dada.com.passwordbox.ui.activitys.MainActivity"></activty>
        <!-- 登录页面 -->
        <activty
            android:name="pb.dada.com.passwordbox.ui.activitys.LoginActivity"
            android:label="LoginActivity">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activty>
    </application>


</manifest>
